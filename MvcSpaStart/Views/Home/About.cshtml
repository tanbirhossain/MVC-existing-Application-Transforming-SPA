@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<table id="table" class="display" >
    <thead>
    <tr>
        <th>Column1</th>
        <th>Column2</th>
        <th>Column3</th>
    </tr>
    </thead>
    <tbody>
        @for (var i = 0; i < 100; i++)
        {
            <tr><td>Some value @DateTime.Now</td><td>Some value @DateTime.Now</td><td>Some value @DateTime.Now</td></tr>
        }
    </tbody>
</table>

@section scripts
{
    <script>
        $(function() {
            $('#table').dataTable();
        });
    </script>
}
